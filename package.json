  "scripts": {
    "start": "bnr start",
    "build": "bnr build",
    "build:client": "webpack --config ./tools/webpack/client.config.js",
    "build:server": "webpack --config ./tools/webpack/server.config.js",
    "start:client": "webpack-serve --config ./tools/webpack/serve.config.js",
    "start:server": "webpack --config ./tools/webpack/server.config.js"
  },
  "betterScripts": {
    "start": {
      "command": "npm-run-all --parallel start:server start:client",
      "env": {
        "NODE_ENV": "development",
        "HOST": "localhost",
        "PORT": 6500
      }
    },
    "build": {
      "command": "npm-run-all --parallel build:server build:client",
      "env": {
        "NODE_ENV": "production",
        "HOST": "localhost",
        "PORT": 6500
      }
    }
  },
